@prefix cro: <http://rhizomik.net/ontologies/copyrightonto#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://rhizomik.net/ontologies/copyrightonto>
  rdf:type owl:Ontology ;
  dc:creator <http://rhizomik.net/~roberto/> ;
  dc:date "2017-10-23"^^xsd:date ;
  dc:description "Copyright Ontology, http://rhizomik.net/ontologies/copyrightonto"@en ;
  dc:license <http://creativecommons.org/licenses/by-sa/4.0/> ;
  vann:preferredNamespacePrefix "cro" ;
  rdfs:label "Copyright Ontology"@en ;
.
cro:AudiovisualRecording
  rdf:type owl:Class ;
  rdfs:label "Audiovisual Recording"@en ;
  rdfs:subClassOf cro:Recording ;
.
cro:Communication
  rdf:type owl:Class ;
  rdfs:comment "The transmission of a work among places at a given time. It is a process performed when the public is not present at the place and or time where the Communication originates. It includes broadcasts, i.e. one to many, but also communications from a place and at a time individually chosen. Examples: a radio broadcast of a sound recording of The Magic Flute or an Internet streaming of the film Les Misérables."@en ;
  rdfs:label "Communication"@en ;
  rdfs:subClassOf schema:Event ;
.
cro:Improvisation
  rdf:type owl:Class ;
  rdfs:comment "The expression in time of a work, which has not been previously materialised in a manifestation, before an audience. Performers or technical methods might be involved in the process. Examples: to play some improvised music in a concert."@en ;
  rdfs:label "Improvisation"@en ;
  rdfs:subClassOf cro:Performance ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cro:Work ;
      owl:onProperty cro:isPerformanceOf ;
    ] ;
.
cro:Instance
  rdf:type owl:Class ;
  rdfs:comment "A copy of a creation object produced for commersialisation, a product. International identification schemes for manifestations: EAN13 (European Article Number) or UPC (Universal Product Code). Examples: a CD of The Magic Flute or a DVD of Les Misérables."@en ;
  rdfs:label "Instance"@en ;
  rdfs:subClassOf schema:CreativeWork ;
.
cro:LiveCommunication
  rdf:type owl:Class ;
  rdfs:label "Live Communication"@en ;
  rdfs:subClassOf cro:Communication ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cro:Performance ;
      owl:onProperty cro:isCommunicationOf ;
    ] ;
.
cro:Manifestation
  rdf:type owl:Class ;
  rdfs:comment "The materialisation of a Work in a concrete medium, i.e. a tangible or digital object. International identification schemes for manifestations: ISBN (International Standard Book Number) for books or ISMN (International Standard Music Number) for printed music. Examples: the printed scores of The Magic Flute or the editions of Les Misérables."@en ;
  rdfs:label "Manifestation"@en ;
  rdfs:subClassOf schema:CreativeWork ;
.
cro:Performance
  rdf:type owl:Class ;
  rdfs:comment "The expression in time of a manifestation of a work before an audience. Performers or technical methods might be involved in the process. Examples: the scenic play of The Magic Flute opera or a screen play of any of Les Misérables films."@en ;
  rdfs:label "Performance"@en ;
  rdfs:subClassOf schema:Event ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cro:LiveCommunication ;
      owl:onProperty cro:hasCommunication ;
    ] ;
.
cro:Recording
  rdf:type owl:Class ;
  rdfs:comment "The materialisation of a performance in a tangible of digital object. International identification schemes for manifestations: ISRC (International Standard Recording Code) for audio and video recordings or ISAN (International Standard Audiovisual Number) for audiovisual works. Examples: a sound recording of The Magic Flute opera or any motion picture of Les Misérables."@en ;
  rdfs:label "Recording"@en ;
  rdfs:subClassOf schema:CreativeWork ;
.
cro:SoundRecording
  rdf:type owl:Class ;
  rdfs:label "Sound Recording"@en ;
  rdfs:subClassOf cro:Recording ;
.
cro:Work
  rdf:type owl:Class ;
  rdfs:comment "The socially shared concept that captures the characteristics that allow the identification of the original creation among all its manifestations. International identification schemes for works: ISTC (International Standard Text Code) for textual works, ISSN (International Standard Serial Number) for serial publications or ISWC (International Standard Musical Work Code) for musical and literary works. Examples: Mozart's The Magic Flute or Victor Hugo's Les Misérables."@en ;
  rdfs:label "Work"@en ;
  rdfs:subClassOf schema:Intangible ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom cro:Improvisation ;
      owl:onProperty cro:hasPerformance ;
    ] ;
.
cro:creationModelProperty
  rdf:type owl:ObjectProperty ;
.
cro:hasCommunication
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Performance
          cro:Recording
          cro:Manifestation
        ) ;
    ] ;
  rdfs:label "has communication"@en ;
  rdfs:range cro:Communication ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isCommunicationOf ;
.
cro:hasDerivation
  rdf:type owl:ObjectProperty ;
  rdf:type owl:TransitiveProperty ;
  rdfs:comment ""^^xsd:string ;
  rdfs:domain cro:Work ;
  rdfs:label "has derivation"@en ;
  rdfs:range cro:Work ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isDerivationOf ;
.
cro:hasInstance
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Manifestation
          cro:Recording
        ) ;
    ] ;
  rdfs:label "has instance"@en ;
  rdfs:range cro:Instance ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isInstanceOf ;
.
cro:hasManifestation
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:comment ""^^xsd:string ;
  rdfs:domain cro:Work ;
  rdfs:label "has manifestation"@en ;
  rdfs:range cro:Manifestation ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isManifestationOf ;
.
cro:hasPerformance
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Manifestation
          cro:Work
        ) ;
    ] ;
  rdfs:label "has performance"@en ;
  rdfs:range cro:Performance ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isPerformanceOf ;
.
cro:hasRecording
  rdf:type owl:InverseFunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Communication
          cro:Performance
        ) ;
    ] ;
  rdfs:label "has recording"@en ;
  rdfs:range cro:Recording ;
  rdfs:subPropertyOf cro:creationModelProperty ;
  owl:inverseOf cro:isRecordingOf ;
.
cro:isCommunicationOf
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain cro:Communication ;
  rdfs:label "is communication of"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Performance
          cro:Recording
          cro:Manifestation
        ) ;
    ] ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
cro:isDerivationOf
  rdf:type owl:ObjectProperty ;
  rdf:type owl:TransitiveProperty ;
  rdfs:domain cro:Work ;
  rdfs:label "is derivation of"@en ;
  rdfs:range cro:Work ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
cro:isInstanceOf
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain cro:Instance ;
  rdfs:label "is instance of"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Manifestation
          cro:Recording
        ) ;
    ] ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
cro:isManifestationOf
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain cro:Manifestation ;
  rdfs:label "is manifestation of"@en ;
  rdfs:range cro:Work ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
cro:isPerformanceOf
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain cro:Performance ;
  rdfs:label "is performance of"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Manifestation
          cro:Work
        ) ;
    ] ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
cro:isRecordingOf
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain cro:Recording ;
  rdfs:label "is recording of"@en ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          cro:Communication
          cro:Performance
        ) ;
    ] ;
  rdfs:subPropertyOf cro:creationModelProperty ;
.
schema:CreativeWork
  rdf:type owl:Class ;
.
schema:Event
  rdf:type owl:Class ;
.
schema:Intangible
  rdf:type owl:Class ;
.
